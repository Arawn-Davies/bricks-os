#----------------------------------------------------------------------------
# PSP
#----------------------------------------------------------------------------
MY_INCL_PATH    +=\

MY_LIBS_PATH    +=\
                -L$(PSPSDK)/lib \

#----------------------------------------------------------------------------
CROSS_COMPILE   =psp-
CFLAGS          +=\

CXXFLAGS        +=\

LFLAGS          +=\
                $(MY_LIBS_PATH) \

#----------------------------------------------------------------------------
# Create .elf file from MY_OBJS + MY_LIBS
$(MY_ELF): $(MY_OBJS) $(MY_LIBS_DEP)
	@echo [CC ] $@
	$(CXX) $(LFLAGS) $(MY_OBJS) $(MY_LIBS) -o $@
