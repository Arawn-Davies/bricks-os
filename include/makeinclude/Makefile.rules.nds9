#----------------------------------------------------------------------------
# DS ARM9
#----------------------------------------------------------------------------
MY_INCL_PATH	+=\

MY_LIBS_PATH	+=\

#----------------------------------------------------------------------------
CROSS_COMPILE   =arm-eabi-
CFLAGS          +=-mcpu=arm9tdmi -mtune=arm9tdmi -fomit-frame-pointer -ffast-math -mthumb -mthumb-interwork -DARM9
CXXFLAGS        +=-mcpu=arm9tdmi -mtune=arm9tdmi -fomit-frame-pointer -ffast-math -mthumb -mthumb-interwork -DARM9
LFLAGS          +=-mthumb -mthumb-interwork -specs=ds_arm9.specs $(MY_LIBS_PATH)

#----------------------------------------------------------------------------
# Create .elf file from MY_OBJS + MY_LIBS
$(MY_ELF): $(MY_OBJS)
	@echo [CC ] $@
	$(CXX) $(LFLAGS) $^ $(MY_LIBS) -o $@
