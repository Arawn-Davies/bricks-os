#----------------------------------------------------------------------------
# DS ARM9
#----------------------------------------------------------------------------
MY_INCL_PATH    +=\

MY_LIBS_PATH    +=\

#----------------------------------------------------------------------------
CROSS_COMPILE   =arm-eabi-
CFLAGS          +=\
                -mcpu=arm9tdmi \
                -mtune=arm9tdmi \
                -fomit-frame-pointer \
                -ffast-math \
                -mthumb \
                -mthumb-interwork \
                -mlong-calls \
                -falign-functions=32 \
                -DNDS \
                -DNDS9 \

CXXFLAGS        +=\
                -mcpu=arm9tdmi \
                -mtune=arm9tdmi \
                -fomit-frame-pointer \
                -ffast-math \
                -mthumb \
                -mthumb-interwork \
                -mlong-calls \
                -falign-functions=32 \
                -DNDS \
                -DNDS9 \

LDFLAGS         +=\
                -mthumb \
                -mthumb-interwork \
                -Wl,-Map,$@.map \

#----------------------------------------------------------------------------
# Create .elf file from MY_OBJS + MY_LIBS
$(MY_ELF): $(MY_OBJS) $(MY_LIBS_DEP)
	@echo [CC ] $@
	$(CXX) -specs=$(BRICKS_ROOT)/include/makeinclude/ds_arm9.specs $(LDFLAGS) $(MY_OBJS) $(MY_LIBS_PATH) $(MY_LIBS) -o $@
