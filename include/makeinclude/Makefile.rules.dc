#----------------------------------------------------------------------------
# Dreamcast
#----------------------------------------------------------------------------
MY_INCL_PATH    +=\

MY_LIBS_PATH    +=\

#----------------------------------------------------------------------------
CROSS_COMPILE   =sh-elf-
CFLAGS          +=\

CXXFLAGS        +=\

LINK_FLAGS      +=\
                -nostdlib \
                $(MY_LIBS_PATH) \

#----------------------------------------------------------------------------
STARTUP_LIBS    =\
                -lgcc \

#----------------------------------------------------------------------------
# Create .elf file from MY_OBJS + MY_LIBS
$(MY_ELF): $(MY_OBJS) $(MY_LIBS_DEP)
	@echo [CC ] $@
	$(CXX) $(LINK_FLAGS) -o $@ $(DEVKITSH)/sh-elf/lib/crt0.o $(MY_OBJS) $(MY_LIBS_PATH) $(MY_LIBS) $(STARTUP_LIBS)
